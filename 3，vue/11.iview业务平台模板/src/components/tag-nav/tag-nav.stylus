.tag-nav
  width 100%
  height 100%
  background-color #F0F0F0
  padding: 0 20px 0 10px;
  .nav-content
    width 100%
    height 100%
    overflow hidden
    position relative
    .content
      display: inline-block;
      position: absolute;
      white-space: nowrap;
      transition left 0.2s ease-out